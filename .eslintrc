{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "allowImportExportEverywhere": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": 0,
    "jsx-a11y/no-static-element-interactions": 1,
    "react/jsx-filename-extension": 0,
    "no-confusing-arrow": 1,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "indent": ["error", 2]
  }
}
